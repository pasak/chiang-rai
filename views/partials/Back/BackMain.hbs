<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{FormControl.Title}}</title>
  <link rel="icon" type="image/x-icon" href="/media/mp-icon.ico">

  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome -->
  <!--link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/fontawesome-free/css/all.min.css"-->
  <script src="https://kit.fontawesome.com/b0a621f9af.js" crossorigin="anonymous"></script>  

  <!-- DataTables -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/datatables-buttons/css/buttons.bootstrap4.min.css">

  <!-- Ionicons -->
  <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Tempusdominus Bootstrap 4 -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
  <!-- iCheck -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/icheck-bootstrap/icheck-bootstrap.min.css">
  <!-- JQVMap -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/jqvmap/jqvmap.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}dist/css/adminlte.min.css">
  <!-- overlayScrollbars -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
  <!-- Daterange picker -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/daterangepicker/daterangepicker.css">
  <!-- summernote -->
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}plugins/summernote/summernote-bs4.min.css">

  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}css/animate.css">
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}css/datepicker.css">
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}css/bootstrap-dropdownhover.css">
  <link rel="stylesheet" href="{{FormControl.BACKEND_URL}}css/back-end.css">

</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  <!-- Preloader -->
  <div class="preloader flex-column justify-content-center align-items-center">
    <img class="animation__shake" src="{{FormControl.Logo}}">
    {{!-- src="{{FormControl.BACKEND_URL}}dist/img/AdminLTELogo.png"  --}}
    {{!-- alt="AdminLTELogo" height="60" width="60"> --}}
  </div>

  <!-- Navbar -->
  <nav class="main-header navbar navbar-expand navbar-white navbar-light">
    <!-- Left navbar links -->
    <ul class="navbar-nav col">
      <li class="nav-item">
        <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
      </li>
      <li class="nav-item d-none d-sm-inline-block">
        <a href="#" class="nav-link">
        {{User.FirstName}} as {{User.Role}} 
        {{#ifCond User.Parent.Name '!=' null }} of {{User.Parent.Name}} {{/ifCond}}
        </a>
    </ul>
    <form class="d-flex" role="search">
      <a href="/manual/MyPrachinTeam-Admin-v1.2.0.pdf" target=_blank>
        คู่มือแอดมิน MyPrachinTeam Manual version 1.2.0</a>
    </form>
  </nav>
  <!-- /.navbar -->

    {{> Back/SideBar}}

    {{{body}}}

  <footer class="main-footer">
    <strong>Copyright &copy; 2024 <a href="#">{{FormControl.Title}}</a>.</strong>
    All rights reserved.
    <div class="float-right d-none d-sm-inline-block">
      <b>Version</b> 1.2
    </div>
  </footer>

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="{{FormControl.BACKEND_URL}}plugins/jquery/jquery.min.js"></script>
<!-- jQuery UI 1.11.4 -->
<script src="{{FormControl.BACKEND_URL}}plugins/jquery-ui/jquery-ui.min.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
  $.widget.bridge('uibutton', $.ui.button)
</script>
<!-- Bootstrap 4 -->
<script src="{{FormControl.BACKEND_URL}}plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- AdminLTE App -->
<script src="{{FormControl.BACKEND_URL}}dist/js/adminlte.js"></script>
<!-- DataTables  & Plugins -->
<script src="{{FormControl.BACKEND_URL}}plugins/datatables/jquery.dataTables.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/jszip/jszip.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/pdfmake/pdfmake.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/pdfmake/vfs_fonts.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-buttons/js/buttons.html5.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-buttons/js/buttons.print.min.js"></script>
<script src="{{FormControl.BACKEND_URL}}plugins/datatables-buttons/js/buttons.colVis.min.js"></script>

<!-- Summernote -->
<script src="{{FormControl.BACKEND_URL}}plugins/summernote/summernote-bs4.min.js"></script>

<script src="{{FormControl.BACKEND_URL}}js/bootstrap-datepicker.js"></script>
<script src="{{FormControl.BACKEND_URL}}js/bootstrap-datepicker-thai.js"></script>
<script src="{{FormControl.BACKEND_URL}}js/locales/bootstrap-datepicker.th.js"></script>

<script>
  $(function () {
    {{#ifCond FormControl.Page '==' 'UserEdit'}}
      $("#BirthDate").datepicker({ autoclose: true });
    {{/ifCond}}
    {{#ifCond FormControl.Page '==' 'PeopleList'}}
      $("#FromDate").datepicker({ autoclose: true });
      $("#ToDate").datepicker({ autoclose: true });
    {{/ifCond}}
    {{#ifCond FormControl.Page '==' 'PeopleEdit'}}
      $("#BirthDate").datepicker({ autoclose: true });
    {{/ifCond}}
    {{#ifCond FormControl.Page '==' 'OperationList'}}
      $("#FromDate").datepicker({ autoclose: true });
      $("#ToDate").datepicker({ autoclose: true });
    {{/ifCond}}
    {{#ifCond FormControl.Page '==' 'OperationEdit'}}
      $("#Date").datepicker({ autoclose: true });
      $("#BeginDate").datepicker({ autoclose: true });
      $("#EndDate").datepicker({ autoclose: true });
    {{/ifCond}}
  })
</script>

</body>
</html>
